// This config is taken from https://github.com/tsconfig/bases/blob/main/bases/recommended.json
{
  "compilerOptions": {
    "target": "ES3",
    "module": "CommonJS",
    "strict": true,
    "noImplicitThis": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}
